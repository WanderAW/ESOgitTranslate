msgctxt "20958740-0-1517"
msgid "ACCEPT"
msgstr ""

#: 249936564-0-1517
msgctxt "249936564-0-1517"
msgid "QUEST: To Loqueach"
msgstr "퀘스트 : 로퀴치로"

#: 20958740-0-1515
msgctxt "20958740-0-1515"
msgid "ACCEPT"
msgstr ""

