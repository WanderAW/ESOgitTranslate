msgstr ""

#: 200879108-0-6891
msgctxt "200879108-0-6891"
msgid "You put red powder into the brazier and it starts to burn."
msgstr ""

#: 228103012-0-6891
msgctxt "228103012-0-6891"
msgid "Put red powder into the brazier."
msgstr ""

#: 200879108-0-6894
msgctxt "200879108-0-6894"
msgid "You put white powder into the brazier and it starts to burn."
msgstr ""

#: 228103012-0-6894
msgctxt "228103012-0-6894"
msgid "Put white powder into the brazier."
msgstr ""

#: 200879108-0-4231
msgctxt "200879108-0-4231"
msgid "We'll be right behind you"
msgstr ""

#: 228103012-0-4231
msgctxt "228103012-0-4231"
msgid "Follow me. We must move fast"
msgstr ""

#: 200879108-0-6893
msgctxt "200879108-0-6893"
msgid "You put black powder into the brazier and it starts to burn."
msgstr ""

#: 228103012-0-6893
